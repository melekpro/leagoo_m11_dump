precision mediump float;attribute vec3 aPosition;attribute vec2 aTexcoord0;attribute vec3 aMorphT1;attribute vec3 aMorphT2;attribute vec3 aMorphT3;attribute vec3 aMorphT4;uniform mat4 aMatrixM;uniform mat4 aMatrixVP;uniform float aWeight1;uniform float aWeight2;uniform float aWeight3;uniform float aWeight4;varying vec2 v_texcoord0;void main(){    vec3 position;    position = aPosition + (aMorphT1 - aPosition) * aWeight1 + (aMorphT2 - aPosition) * aWeight2 + (aMorphT3 - aPosition) * aWeight3 + (aMorphT4 - aPosition) * aWeight4;    gl_Position = aMatrixVP*aMatrixM*vec4(position.xyz,1.0);    v_texcoord0    = aTexcoord0;}